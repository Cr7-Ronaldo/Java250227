package cho02.sec01;

public class abc {

	public static void main(String[] args) {
		float a = 10;
	    float b = 20;
	    System.out.println("before");
	    System.out.println("a = "+a + ", " + "b ="+ b);
	    float c = a;
	    a=b;
	    b=c;
	    System.out.println("after");
	    System.out.println("a = "+a + ", " + "b ="+ b);
	    // double > float > long > int > short > byte
	    
	    
	}
}
